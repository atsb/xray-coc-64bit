			<texture>freedom_big</texture>
		</image_0>
		<image_1 x="292" y="42" width="259" height="234" stretch="1">
			<texture>merc_big</texture>
		</image_1>

		<text_desc x="35" y="303" width="526" height="144" complex_mode="1">
